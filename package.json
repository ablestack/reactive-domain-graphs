{
  "private": true,
  "name": "@ablestack/reactive-domain-objects",
  "version": "0.6.0",
  "description": "A suite of packages to facilitate building and running graphs of Reactive Domain Objects",
  "author": "jamshally",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ablestack/reactive-domain-objects"
  },
  "keywords": [
    "json",
    "typescript",
    "graphql",
    "react",
    "reactjs",
    "state-management",
    "domain-models",
    "mobx",
    "mobx-state-tree",
    "mox-react",
    "observable",
    "reactive",
    "json-data",
    "idempotent",
    "imperative",
    "reactive"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "tsc --build --verbose",
    "rebuild": "tsc --build --force --verbose",
    "pub": "npm run build && lerna publish --forcePublish",
    "lpub": "npm run build && yalc publish packages/rdo/dist/src --force --no-script && yalc publish packages/rdo-apollo-mobx-connector/dist/src --force --no-script",
    "lpush": "npm run build && yalc push packages/rdo/dist/src --force --no-script && yalc push packages/rdo-apollo-mobx-connector/dist/src --force --no-script",
    "test": "npm run build && jest"
  },
  "devDependencies": {
    "@ablestack/rdo": "^0.6.0",
    "@ablestack/rdo-apollo-mobx-connector": "^0.6.0",
    "babel-jest": "^26.0.1",
    "dotenv": "^8.2.0",
    "jest": "^26.0.1",
    "lerna": "^3.20.0",
    "mobx": "^5.15.0",
    "path": "^0.12.7",
    "shx": "^0.3.3",
    "ts-jest": "^26.0.0",
    "ts-node": "~7.0.0",
    "tslib": "2.0.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@types/jest": "^25.2.3"
  }
}
