    "@types/lodash": "^4.14.154",
    "babel-jest": "^26.0.1",
    "dotenv": "^8.2.0",
    "jest": "^26.0.1",
    "ts-jest": "^26.0.0",
    "ts-node": "~7.0.0",
    "tslib": "2.0.0",
    "typescript": "^3.9.3"
